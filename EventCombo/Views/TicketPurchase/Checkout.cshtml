    <!--****************************-->
    <div class="row margin-1em-bottom">
      <div class="c12 margin-1em-bottom"><h2 class="red-heading">Payment Method</h2></div>
      <div class="c12 paymentSpace">
        <md-radio-group ng-model="paymentMethod" class="paymentMethod" ng-init="paymentMethod='creditcard'">
          <md-radio-button ng-value="'creditcard'" class="md-primary margin-2em-right" aria-label="Credit Card">
            <img src="~/Images/AMaterial/credit-card.jpg">
          </md-radio-button>
          <md-radio-button ng-value="'paypal'" class="md-primary" aria-label="PayPal">
            <img src="~/Images/AMaterial/paypal.jpg">
          </md-radio-button>
        </md-radio-group>
      </div>
      @if (!String.IsNullOrEmpty(Model.BaseUserId))
    {
        <div class="c12 paymentSpace logColor">Hi @(Model.BaseUserName), Not you ? <a onclick="alert('Test Signout')">Sign Out</a></div>
      }
      else
      {
        <div class="c12 paymentSpace logColor">Have you used Eventcombo before ? <a onclick="alert('Test Login')">Log In</a></div>
      }
    </div>
    <!--****************************-->
    <form name="creditcardForm" novalidate>
      <div class="row" ng-show="paymentMethod=='creditcard'">
        <div class="c6 paymentSpace">
          <md-input-container class="block45">
            <label>First Name</label>
            <input name="checkoutFirstName" ng-model="checkoutFirstName" ng-keypress="submitted=false" ng-required="true">
            <div ng-messages="creditcardForm.checkoutFirstName.$error" role="alert">
              <div ng-message="required">Enter first name</div>
            </div>
          </md-input-container>
          <md-input-container class="block45">
            <label>Last Name</label>
            <input name="checkoutLastName" ng-model="checkoutLastName" ng-keypress="submitted=false" ng-required="true">
            <div ng-messages="creditcardForm.checkoutLastName.$error" role="alert">
              <div ng-message="required">Enter last name</div>
            </div>
          </md-input-container>
          <md-input-container class="block100">
            <label>Email</label>
            <input type="email" name="checkoutEmail" ng-model="checkoutEmail" ng-pattern="/^.+@("@").+\..+$/" ng-keypress="submitted=false" ng-required="true" />
            <div ng-messages="creditcardForm.checkoutEmail.$error" role="alert">
              <div ng-message="required">Enter your email address</div>
              <div ng-message="pattern">Use valid email format</div>
            </div>
          </md-input-container>
          <md-input-container class="block100">
            <label>Card Number</label>
            <input credit-card name="checkoutCardNumber" ng-model="checkoutCardNumber" ng-minlength="15" ng-required="true">
            <img class="cardType" ng-show="crCardType=='mastercard'" src="~/Images/AMaterial/master-card-icon.jpg">
            <img class="cardType" ng-show="crCardType=='visa'" src="~/Images/AMaterial/visa-icon.jpg">
            <img class="cardType" ng-show="crCardType=='discover'" src="~/Images/AMaterial/discover-icon.jpg">
            <img class="cardType" ng-show="crCardType=='amex'" src="~/Images/AMaterial/amex-icon.jpg">
            <div ng-messages="creditcardForm.checkoutCardNumber.$error" role="alert">
              <div ng-message="required">Enter card number</div>
              <div ng-message="minlength">Enter a valid card number</div>
              <div ng-message="crCardMessage">We only accept MasterCard, Visa and Discover</div>
              <div ng-message="amexMessage">We do not accept AMEX</div>
            </div>
          </md-input-container>
          <md-input-container class="block100">
            <label>Name on Card</label>
            <input name="checkoutNameOnCard" ng-model="checkoutNameOnCard" ng-required="true">
            <div ng-messages="creditcardForm.checkoutNameOnCard.$error" role="alert">
              <div ng-message="required">Enter name on card</div>
            </div>
          </md-input-container>
          <md-input-container class="block100">
            <label>Expiration Date</label>
            <input expiration-date name="checkoutExpirationDate" ng-model="checkoutExpirationDate" ng-pattern="/^((0[1-9])|(1[0-2]))\/([0-9]{2,4})$/" ng-required="true">
            <div ng-messages="creditcardForm.checkoutExpirationDate.$error" role="alert">
              <div ng-message="required">Enter expiration date</div>
              <div ng-message="pattern">Enter a valid expiration date in MM/YY format</div>
            </div>
          </md-input-container>
          <md-input-container class="block100">
            <label>Security Code</label>
            <input security-code name="checkoutSecurityCode" ng-model="checkoutSecurityCode" ng-pattern="/^[0-9]{3,4}$/" ng-required="true">
            <span class="cvv_info">
              <i class="material-icons">info</i>
              <span class="cv_card"><img ng-src="~/Images/AMaterial/cv_card.gif"></span>
            </span>
            <div ng-messages="creditcardForm.checkoutSecurityCode.$error" role="alert">
              <div ng-message="required">Enter security code</div>
              <div ng-message="pattern">Enter a valid security code</div>
            </div>
          </md-input-container>
        </div>

        <div class="c6 paymentSpace">
          <md-checkbox ng-model="checkoutBilling" ng-init="checkoutBilling=true" aria-label="Billing Address" class="blueCheckbox">Billing address is the same as shipping</md-checkbox>
          <div class="block100"><strong>Billing Information</strong></div>
          <md-input-container class="block30">
            <label>Zip Code</label>
            <input name="checkoutBillingZipCode" ng-model="checkoutBillingZipCode" ng-required="true">
            <div ng-messages="creditcardForm.checkoutBillingZipCode.$error" role="alert">
              <div ng-message="required">Enter billing zip</div>
            </div>
          </md-input-container>
          <md-input-container class="block30">
            <label>City</label>
            <input name="checkoutBillingCity" ng-model="checkoutBillingCity" ng-required="true">
            <div ng-messages="creditcardForm.checkoutBillingCity.$error" role="alert">
              <div ng-message="required">Enter billing city</div>
            </div>
          </md-input-container>
          <md-input-container class="block30">
            <label>State</label>
            <md-select name="checkoutBillingState" ng-model="checkoutBillingState" ng-required="true">
              <md-option ng-repeat="state in [1,2,3]">
                {{state}}
              </md-option>
            </md-select>
            <div ng-messages="creditcardForm.checkoutBillingState.$error" role="alert">
              <div ng-message="required">Enter billing state</div>
            </div>
          </md-input-container>
          <md-input-container class="block100">
            <label>Billing Street Address</label>
            <input name="checkoutBillingAddress" ng-model="checkoutBillingAddress" ng-required="true">
            <div ng-messages="creditcardForm.checkoutBillingAddress.$error" role="alert">
              <div ng-message="required">Enter billing street address</div>
            </div>
          </md-input-container>
          <md-input-container class="block100">
            <label>Apt., Floor, Unit (optional)</label>
            <input ng-model="checkoutBillingAptFloorUnit">
          </md-input-container>
          <md-input-container class="block100">
            <label>Phone Number</label>
            <input tel-mask name="checkoutPhone" ng-model="checkoutPhone" ng-pattern="/^[0-9]{3}-[0-9]{3}-[0-9]{4,5}$/" ng-required="true" />
            <div ng-messages="creditcardForm.checkoutPhone.$error">
              <div ng-message="required">Enter phone number</div>
              <div ng-message="pattern">Enter a valid phone number - ###-###-####</div>
            </div>
          </md-input-container>
          <div class="block100" ng-hide="checkoutBilling"><strong>Shipping Information</strong></div>
          <md-input-container class="block30" ng-hide="checkoutBilling">
            <label>Zip Code</label>
            <input name="checkoutShippingZipCode" ng-model="checkoutShippingZipCode" ng-required="true">
            <div ng-messages="creditcardForm.checkoutShippingZipCode.$error" role="alert">
              <div ng-message="required">Enter shipping zip</div>
            </div>
          </md-input-container>
          <md-input-container class="block30" ng-hide="checkoutBilling">
            <label>City</label>
            <input name="checkoutShippingCity" ng-model="checkoutShippingCity" ng-required="true">
            <div ng-messages="creditcardForm.checkoutShippingCity.$error" role="alert">
              <div ng-message="required">Enter shipping city</div>
            </div>
          </md-input-container>
          <md-input-container class="block30" ng-hide="checkoutBilling">
            <label>State</label>
            <md-select name="checkoutShippingState" ng-model="checkoutShippingState" ng-required="true">
              <md-option ng-repeat="state in [1,2,3]">
                {{state}}
              </md-option>
            </md-select>
            <div ng-messages="creditcardForm.checkoutBillingState.$error" role="alert">
              <div ng-message="required">Enter shipping state</div>
            </div>
          </md-input-container>
          <md-input-container class="block100" ng-hide="checkoutBilling">
            <label>Shipping Street Address</label>
            <input name="checkoutShippingAddress" ng-model="checkoutShippingAddress" ng-required="true">
            <div ng-messages="creditcardForm.checkoutShippingAddress.$error" role="alert">
              <div ng-message="required">Enter shipping street address</div>
            </div>
          </md-input-container>
          <md-input-container class="block100" ng-hide="checkoutBilling">
            <label>Apt., Floor, Unit (optional)</label>
            <input ng-model="checkoutShippingAptFloorUnit">
          </md-input-container>
        </div>

        <div class="c12 text-right margin-1em-top">
          <md-input-container class="block100" style="margin:0;">
            <md-checkbox ng-model="termsConditions" ng-init="termsConditions=true" ng-checked="true" aria-label="I agree to the Terms and Conditions" class="blueCheckbox" required>I agree to the Eventcombo <a href="/ec/terms" target="_blank">Terms and Conditions</a> for purchasing or registering for this event and its deals.</md-checkbox>
          </md-input-container>
        </div>
        <div class="c12 text-right">
          <md-button class="chkBack" onClick="history.go(-1); return false;">GO BACK</md-button>
          <md-button class="chkSubmit" type="submit" ng-click="creditcardSubmit('creditcardForm')">SUBMIT PAYMENT</md-button>
        </div>

      </div>
    </form>
