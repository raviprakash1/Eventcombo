                            if (elid.trim() == "id_Discount") {
                                $('#id_lbl_discount-' + el).css('display', 'block');
                                $('#id_lbl_discount-' + el).html('Please Enter Number');
                            }
                           
                        }


                    }
                }
            });
        });
    }
