                            Sale_Start_Date: $('#id_salestart-' + i).val(),
                            Sale_Start_Time: $('#id_hdsaletimestart-' + i).val(),
                            Sale_End_Date: $('#id_saleend-' + i).val(),
                            Sale_End_Time: $('#id_hdsaletimeend-' + i).val(),
