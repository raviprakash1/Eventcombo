                            T_Mark_SoldOut: parseInt(($('#id_marksoldout-' + i).is(":checked") == true ? '1' : '0')),
                            T_Displayremaining: ($('#id_displayremaining-' + i).is(":checked") == true ? '1' : '0'),
                            Fees_Type: $('#id_include_ticket_fees-' + i).val(),
