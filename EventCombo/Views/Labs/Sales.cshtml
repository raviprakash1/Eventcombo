@{
    Layout = null;
}


<!DOCTYPE html>
<html lang="en-US">
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>EventCombo Labs &#8211; Committed to advancing your work experience</title>
    <script src="~/Scripts/jquery-2.1.4.min.js"></script>
    <script src="~/Scripts/AMaterial/jquery-1.12.3.min.js"></script>
    @Scripts.Render("~/Scripts/AMaterial/angularHeader1")
    @Scripts.Render("~/Scripts/AMaterial/angularHeader2")
    @Scripts.Render("~/Scripts/AMaterial/angularHeader3")
    @Scripts.Render("~/Scripts/AMaterial/angularHeader4")
    @Scripts.Render("~/Scripts/AMaterial/angularHeader5")
    <script src="https://maps.googleapis.com/maps/api/js?key= AIzaSyDTZMtI8hnNjf0va7dL5SYp7fo-hNuMdOs&amp;v=3.exp&amp;sensor=true&amp;libraries=places"></script>
    <link href="~/Content/AMaterial/angular-material.css" rel="stylesheet" />
    <script>
    $(document).ready(function () {
      $(".slider-readmore").click(function () {
        $('html, body').animate({
          scrollTop: $("#labs").offset().top - 80
        }, 2000);
      });
	    $("#menu-item-48").click(function () {
        $('html, body').animate({
          scrollTop: $("#team").offset().top - 50
        }, 2000);
      });
	    $("#menu-item-30").click(function () {
        $('html, body').animate({
          scrollTop: $("#labs").offset().top - 50
        }, 2000);
      });

    });
    </script>
    <style type="text/css">
        .cntButton {
            background: none;
            color: #0077c8 !important;
        }
    </style>

    <link rel='stylesheet' id='himalayas-google-fonts-css' href='//fonts.googleapis.com/css?family=Crimson+Text%3A700%7CRoboto%3A400%2C700%2C900%2C300&#038;ver=4.6.1' type='text/css' media='all' />
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/labs.css" rel="stylesheet" />
    <meta name="generator" content="WordPress 4.6.1" />


    <!-- EventCombo Labs Custom Styles -->

</head>

<body class="home blog" ng-app="EventComboApp">
    <div id="page" class="hfeed site">
        <header id="masthead" class="site-header clearfix" role="banner">
            <div class="header-wrapper clearfix stick non-transparent">
                <div class="tg-container">
                    <div class="logo">
                        <a href="index.html" title="EventCombo Labs" rel="home"><img src="~/Images/labslogo-1.png" alt="EventCombo Labs"></a>
                    </div> <!-- logo-end -->
                    <div id="header-text" class="screen-reader-text">
                        <h1 id="site-title">
                            <a href="http://www.spread07.com/eventcombo/" title="EventCombo Labs" rel="home">EventCombo Labs</a>
                        </h1>
                        <p id="site-description">Committed to advancing your work experience</p>
                    </div><!-- #header-text -->

                    <div class="menu-search-wrapper">

                        <!-- home-search-end -->

                        <nav id="site-navigation" class="main-navigation" role="navigation" ng-controller="FooterController">
                            <p class="menu-toggle hide"></p>
                            <div class="menu-primary-container">
                                <ul id="menu-labs-menu" class="menu">
                                    <li id="menu-item-30" class="menu-item menu-item-type-custom menu-item-object-custom current-menu-item current_page_item menu-item-home menu-item-30"><a href="@Url.Action("Index","Labs")#Labs">Labs</a></li>
                                    <li id="menu-item-48" class="menu-item menu-item-type-custom menu-item-object-custom current-menu-item current_page_item menu-item-home menu-item-48"><a href="@Url.Action("Index","Labs")#About">about</a></li>
                                    <li id="menu-item-31" class="menu-item menu-item-type-custom menu-item-object-custom current-menu-item current_page_item menu-item-home menu-item-31"><a href="#" ng-click="contactEventcombo($event)">Contact</a></li>
                                </ul>
                            </div>
                        </nav> <!-- nav-end -->
                    </div> <!-- Menu-search-wrapper end -->
                </div><!-- tg-container -->
            </div><!-- header-wrapepr end -->



        </header>


        <div id="content" class="site-content">
            <section id="himalayas_service_widget-2" class="widget widget_service_block clearfix">
                <div id="labs">
                    <div class="section-wrapper">
                        <div class="tg-container">

                            <div class="section-title-wrapper">
                                <h2 class="main-title inner">EVENTCOMBO SALES LAB</h2>
                                <p>Seeking a motivated sales/business development agent to join our dynamic tech startup company and be a part of our growing team. You will collaborate with the team and report to the VP of Business Development to assist in the management and development aspect of the sales and business development strategy. You will concurrently learn about business development, the sales process, and general business strategy. Full time employment at the end of the internship is possible based on your performance.</p>
                                <div class="nav-full">
                                    <ul class="nav-text">
                                        <h1>Wins</h1>
                                        <li>Candidate should be an efficient communicator and capable of engaging with decision makers</li>
                                        <li>Candidate will be responsible for identifying potential leads and targeting them with a proposed solution</li>
                                        <li>Candidate will be able to maintain and exceed expectations with relationships</li>
                                        <li>Candidate will develop experience by working with a dynamic sales team in a real business environment</li>
                                    </ul>
                                    <ul class="nav-text">
                                        <h1>Responsibilities</h1>
                                        <li>Identify partnerships based on exciting opportunities locally and nationally in the event space  </li>
                                        <li>Secure engagement with leads through business driven initiatives </li>
                                        <li>Learn about the sales process, funnel and what it takes to turn a prospect into a customer</li>
                                        <li>Learn and develop inbound marketing strategies to increase business   </li>
                                        <li>Identify leads and compile reports to follow up</li>
                                        <li>Work with our account managers to identify opportunities to involve clients </li>

                                    </ul>
                                    <ul class="nav-text">
                                        <h1>Requirements</h1>
                                        <li>Collaborative team player who can take direction well and is interested in learning about business development, sales and events. </li>
                                        <li>Excellent written communication skills  </li>
                                        <li>Highly motivated and hard worker</li>
                                        <li>Detail oriented and extremely organized </li>
                                        <li>Computer skills and navigating different platforms</li>
                                        <li>Interested in sales, business development, and events</li>

                                    </ul>
                                </div>
                            </div>


                        </div>
                    </div>
                </div>
            </section>
        </div>

        <footer id="colophon" class="footer-with-widget footer-layout-two">

            <div class="footer-logo">
                <a href="index.html" title="EventCombo Labs" rel="home"><img src="~/Images/labslogo-1.png" alt="EventCombo Labs"></a>
            </div>

            <div id="bottom-footer">
                <div class="tg-container">

                    <div class="copyright">
                        <span class="copyright-text">Copyright &copy; 2016 <a href="@Url.Action("Index","Labs")" target="_blank" title="EventCombo Labs">EventCombo Labs</a>. Jump to<a href="https://www.eventcombo.com/" target="#">  Eventcombo.com</a>.</span>
                    </div>
                    <div class="footer-nav">
                    </div>
                </div>
            </div>
        </footer>
        <a href="#" class="scrollup"><i class="fa fa-angle-up"> </i> </a>

    </div> <!-- #Page -->
    <script type="text/ng-template" id="eventcombo.tmpl.html">
        <md-dialog aria-label="Contact Eventcombo" data-ng-app="eventComboApp">
            <form name="eventcomboForm" novalidate>
                <md-dialog-content>
                    <div class="md-dialog-content">
                        <h2 class="cntTitle">Need to get in touch? We're listening.</h2>
                        <p class="cntSubtitle">There's a good chance we have an answer in our <a href="@Url.Action("BusinessPage","BusinessPages",new { PageNameUrl="ticket-purchase-faq" })">F.A.Q.</a> <br />Didn't find what you're looking for? Complete the form below and we'll get back to you right away.</p>
                        <md-input-container class="md-block margin-2em-bottom max350">
                            <label>Name</label>
                            <input md-no-asterisk name="cntName" ng-model="cntName" required ng-keypress="submitted=false">
                            <div ng-messages="eventcomboForm.cntName.$error">
                                <div ng-message="required">Please enter your Name!</div>
                            </div>
                        </md-input-container>
                        <md-input-container class="md-block margin-2em-bottom max350">
                            <label>Email</label>
                            <input type="email" required name="cntEmail" ng-model="cntEmail" ng-pattern="/^.+@(" @").+\..+$/" ng-keypress="submitted=false" />
                            <div ng-messages="eventcomboForm.cntEmail.$error" role="alert">
                                <div ng-message="required">Please enter your Email Address!</div>
                                <div ng-message="pattern">Please use valid email format!</div>
                            </div>
                        </md-input-container>
                        <md-input-container class="md-block max350">
                            <label>Phone</label>
                            <input tel-mask name="cntPhone" ng-model="cntPhone" ng-pattern="/^[0-9]{3}-[0-9]{3}-[0-9]{4,5}$/" />
                            <div ng-messages="eventcomboForm.cntPhone.$error">
                                <div ng-message="pattern">Please enter a valid phone number pattern - ###-###-####</div>
                            </div>
                        </md-input-container>
                        <md-input-container class="width160">
                            <label>Category</label>
                            <md-select ng-model="cntCategory" ng-change="getSubCategories()">
                                <md-option ng-repeat="category in Categories">
                                    {{category}}
                                </md-option>
                            </md-select>
                        </md-input-container>
                        <md-input-container class="width160">
                            <label>Sub Category</label>
                            <md-select ng-model="cntSubCategory">
                                <md-option ng-repeat="subcategory in SubCategories">
                                    {{subcategory}}
                                </md-option>
                            </md-select>
                        </md-input-container>
                        <md-input-container class="md-block">
                            <label>Message</label>
                            <textarea ng-model="cntMessage" rows="3" md-select-on-focus></textarea>
                        </md-input-container>
                        <md-dialog-actions layout="row">
                            <md-button class="cntButton" ng-click="cancel()">Cancel</md-button>
                            <md-button class="cntButton" type="submit" ng-click="sendContactMessageEventcombo('eventcomboForm')">Submit</md-button>
                        </md-dialog-actions>
                    </div>
                </md-dialog-content>
            </form>
        </md-dialog>
    </script>
    <script type='text/javascript' src='~/Scripts/jquery.nav.js'></script>
    <script type='text/javascript' src='~/Scripts/himalayas.js'></script>

</body>
</html>