
@{
    ViewBag.Title = "Dashboard";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section id="content_wrapper">

    <!-- Start: Topbar-Dropdown -->

    <header id="topbar" class="alt">
        <div class="topbar-left">
            <ol class="breadcrumb">

                <li class="crumb-icon">
                    <a href="dashboard.html">
                        <span class="glyphicon glyphicon-home"></span>
                    </a>
                </li>
                <li class="crumb-active">
                    Dashboard
                </li>
            </ol>
        </div>

    </header>

    <section id="content" class="table-layout animated fadeIn">

        <!-- begin: .tray-center -->
        <div class="tray tray-center">

            <!-- dashboard tiles -->
            <div class="row">
                <div class="col-sm-3 col-xl-3">
                    <div class="panel panel-tile text-center br-a br-grey">
                        <div class="panel-body bg-primary light">
                            <i class="fa fa-tachometer text-muted fs70 mt10"></i>

                        </div>
                        <div class="panel-footer br-t p12">
                            <span class="fs15">
                                <b><a href="#">Dashboard</a></b>
                            </span>
                        </div>
                    </div>
                </div>
                <div class="col-sm-3 col-xl-3">
                    <div class="panel panel-tile text-center br-a br-grey">
                        <div class="panel-body bg-system light">
                            <i class="fa fa-user text-muted fs70 mt10"></i>
                        </div>
                        <div class="panel-footer br-t p12">
                            <span class="fs15">
                                <b><a href="#">User</a></b>
                            </span>
                        </div>
                    </div>
                </div>
                <div class="col-sm-3 col-xl-3">
                    <div class="panel panel-tile text-center br-a br-grey">
                        <div class="panel-body bg-danger light">
                            <i class="fa fa-lemon-o text-muted fs70 mt10"></i>
                        </div>
                        <div class="panel-footer br-t p12">
                            <span class="fs15">
                                <b><a href="#">Event</a></b>
                            </span>
                        </div>

                    </div>
                </div>

                <div class="col-sm-3 col-xl-3">
                    <div class="panel panel-tile text-center br-a br-grey">
                        <div class="panel-body bg-warning light">
                            <i class="fa fa-ticket text-muted fs70 mt10"></i>
                        </div>
                        <div class="panel-footer br-t p12">
                            <span class="fs15">
                                <b><a href="#">Ticket</a></b>
                            </span>
                        </div>
                    </div>
                </div>
                <div class="col-sm-3 col-xl-3">
                    <div class="panel panel-tile text-center br-a br-grey">
                        <div class="panel-body bg-dark">
                            <i class="fa fa-tags text-muted fs70 mt10"></i>
                        </div>
                        <div class="panel-footer br-t p12">
                            <span class="fs15">
                                <b><a href="#">Deals</a></b>
                            </span>
                        </div>
                    </div>
                </div>
                <div class="col-sm-3 col-xl-3">
                    <div class="panel panel-tile text-center br-a br-grey">
                        <div class="panel-body bg-alert light">
                            <i class="fa fa-sun-o text-muted fs70 mt10"></i>
                        </div>
                        <div class="panel-footer br-t p12">
                            <span class="fs15">
                                <b><a href="#">Template</a></b>
                            </span>
                        </div>
                    </div>
                </div>



            </div>
        </div>
    </section>
    <!-- Begin: Page Footer -->
    <footer id="content-footer" class="affix">
        <div class="row">
            <div class="col-md-6">
                <span class="footer-legal">© 2015 Eventcombo</span>
            </div>
            <div class="col-md-6 text-right">
                <span class="footer-meta">Eventcombo<b></b></span>
                <a href="#content" class="footer-return-top">
                    <span class="fa fa-arrow-up"></span>
                </a>
            </div>
        </div>
    </footer>
    <!-- End: Page Footer -->
    <!-- End: Content-Wrapper -->
</section>
